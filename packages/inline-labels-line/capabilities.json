  "dataViewMappings": [
    {
      "conditions": [
        {
          "xAxis": { "max": 2 },
          "group": { "max": 0 },
          "legend": { "max": 1 },
          "values": { "max": 1 },
          "values2": { "max": 1 }
        }
      ],
      "categorical": {
        "categories": {
          "select": [
            { "for": { "in": "xAxis" } },
            { "for": { "in": "legend" } }
          ],
          "dataReductionAlgorithm": {
            "top": { "count": 30000 }
          }
        },
        "values": {
          "select": [
            { "for": { "in": "values" } },
            { "for": { "in": "values2" } }
          ]
        }
      }
    },
    {
      "conditions": [
        {
          "xAxis": { "max": 2 },
          "group": { "min": 1, "max": 1 },
          "legend": { "max": 1 },
          "values": { "max": 1 },
          "values2": { "max": 1 }
        }
      ],
      "categorical": {
        "categories": {
          "select": [
            { "for": { "in": "xAxis" } },
            { "for": { "in": "legend" } }
          ],
          "dataReductionAlgorithm": {
            "top": { "count": 30000 }
          }
        },
        "values": {
          "group": {
            "by": "group",
            "select": [
              { "for": { "in": "values" } },
              { "for": { "in": "values2" } }
            ]
          }
        }
      }
    }
  ],
  "supportsHighlight": true,
